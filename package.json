{
  "name": "vigour-example",
  "version": "1.0.16",
  "description": "Example vigour app",
  "scripts": {
    "test": "gaston test",
    "build": "gaston build -s src -o . -i",
    "all": "npm run build && ./node_modules/.bin/wrapper build",
    "web": "npm run build && ./node_modules/.bin/wrapper build --native.selectedPlatforms web",
    "ios": "npm run build && ./node_modules/.bin/wrapper build --native.selectedPlatforms ios",
    "android": "npm run build && ./node_modules/.bin/wrapper build --native.selectedPlatforms android",
    "samsungtv": "npm run build && ./node_modules/.bin/wrapper build --native.selectedPlatforms samsungtv",
    "webostv": "npm run build && ./node_modules/.bin/wrapper build --native.selectedPlatforms webostv",
    "netcasttv": "npm run build && ./node_modules/.bin/wrapper build --native.selectedPlatforms netcasttv",
    "chromecastweb": "npm run build && ./node_modules/.bin/wrapper build --native.selectedPlatforms chromecastweb",
    "release": "ferry --release --mergeFiles '[\"./package.json\",\"./.package.json\"]'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vigour-io/vigour-example.git"
  },
  "keywords": [
    "vigour",
    "example"
  ],
  "author": "Vigour",
  "license": "MIT",
  "contributors": [
    "Shawn Inder <shawn@vigour.io>",
    "Alexander Van Der Werff <alex@vigour.io>",
    "Michiel Van Liempt <michiel@vigour.io>",
    "Renan Carvalho <renan@vigour.io>",
    "Andre Padez <andre@vigour.io>"
  ],
  "engines": {},
  "bugs": {
    "url": "https://github.com/vigour-io/vigour-example/issues"
  },
  "homepage": "https://github.com/vigour-io/vigour-example",
  "dependencies": {
    "vigour-element": "^0.0.3",
    "vigour-env": "^1.0.1",
    "vigour-status-bar": "^1.0.4",
    "vigour-uikit": "^0.0.1"
  },
  "devDependencies": {
    "vigour-wrapper": "^2.0.0",
    "vigour-ferry": "^3.0.0"
  },
  "vigour": {
    "wrapper": {
      "web": "build.html",
      "assets": {
        "build.html": true,
        "build.js": true,
        "build.css": true,
        "assets": "*"
      }
    },
    "native": {
      "platforms": {
        "web": {
          "appIndexPath": "build.html",
          "assets": {
            "build.html": true,
            "build.js": true,
            "build.css": true
          }
        },
        "ios": {
          "productName": "Vigour Example",
          "organizationName": "Vigour",
          "organizationIdentifier": "io.vigour.example",
          "buildNumber": "1",
          "appIndexPath": "build.html",
          "appIcon": "assets/img/appIcon.png",
          "splashScreen": "assets/img/splash.png",
          "NSAllowsArbitraryLoads": true,
          "interfaceOrientations": {
            "iPhone": [
              "Portrait"
            ],
            "iPad": [
              "Portrait",
              "LandscapeLeft",
              "UpsideDown",
              "LandscapeRight"
            ]
          },
          "assets": {
            "build.html": true,
            "build.js": true,
            "build.css": true
          }
        },
        "android": {
          "productName": "Vigour Example",
          "applicationId": "io.vigour.test",
          "appIndexPath": "build.html",
          "appIcon": "assets/img/appIcon.png",
          "splashScreen": "assets/img/splash.png",
          "splashDuration": 1500,
          "keystoreFile": "example.keystore",
          "keystorePassword": "shmexample",
          "keystoreAlias": "blabla",
          "run": true,
          "debug": true,
          "version": "2.5",
          "versionCode": 36,
          "assets": {
            "build.html": true,
            "build.js": true,
            "build.css": true
          }
        },
        "samsungtv": {
          "productName": "Vigour Samsung Example",
          "xmlConfig": {
            "category": "video",
            "name": "Vigour",
            "author": "Vigour",
            "description": "Vigour",
            "version": "1.0.0",
            "icons": {
              "ThumbIcon": "assets/img/appIcon.png",
              "BigThumbIcon": "assets/img/appIcon.png",
              "ListIcon": "assets/img/appIcon.png",
              "BigListIcon": "assets/img/appIcon.png"
            }
          },
          "assets": {
            "build.html": true,
            "build.js": true,
            "build.css": true
          }
        },
        "chromecastweb": {},
        "netcasttv": {
          "assets": {
            "build.html": true,
            "build.js": true,
            "build.css": true
          }
        },
        "webostv": {
          "productName": "Vigour LG Example",
          "appinfo": {
            "domain": "com.vigour12.io",
            "name": "Vigour Example new build"
          },
          "assets": {
            "build.html": true,
            "build.js": true,
            "build.css": true,
            "assets": "*"
          }
        }
      }
    },
    "ferry": {
      "web": "build.html",
      "assets": {
        "build.html": true,
        "build.js": true,
        "build.css": true
      }
    }
  },
  "gaston": {
    "index-path": "src/index.html",
    "source-maps": false,
    "require-paths": {}
  }
}
