{
  "name": "vigour-example",
  "version": "1.0.19",
  "description": "Example vigour app",
  "scripts": {
    "test": "gaston test",
    "prebuild": "gaston build -s src -o . -i",
    "build": "npm run prebuild && ./node_modules/.bin/wrapper build --native.builds false",
    "all": "npm run prebuild && ./node_modules/.bin/wrapper build",
    "web": "npm run prebuild && ./node_modules/.bin/wrapper build --native.selectedPlatforms web",
    "ios": "npm run prebuild && ./node_modules/.bin/wrapper build --native.selectedPlatforms ios",
    "android": "npm run prebuild && ./node_modules/.bin/wrapper build --native.selectedPlatforms android",
    "samsungtv": "npm run prebuild && ./node_modules/.bin/wrapper build --native.selectedPlatforms samsungtv",
    "webostv": "npm run prebuild && ./node_modules/.bin/wrapper build --native.selectedPlatforms webostv",
    "netcasttv": "npm run prebuild && ./node_modules/.bin/wrapper build --native.selectedPlatforms netcasttv",
    "chromecastweb": "npm run prebuild && ./node_modules/.bin/wrapper build --native.selectedPlatforms chromecastweb",
    "release": "ferry --release --mergeFiles '[\"./package.json\",\"./.package.json\"]'",
    "dist": "gaston dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vigour-io/vigour-example.git"
  },
  "keywords": [
    "vigour",
    "example"
  ],
  "author": "Vigour",
  "license": "MIT",
  "contributors": [
    "Shawn Inder <shawn@vigour.io>",
    "Alexander Van Der Werff <alex@vigour.io>",
    "Michiel Van Liempt <michiel@vigour.io>",
    "Renan Carvalho <renan@vigour.io>",
    "Andre Padez <andre@vigour.io>"
  ],
  "engines": {},
  "bugs": {
    "url": "https://github.com/vigour-io/vigour-example/issues"
  },
  "homepage": "https://github.com/vigour-io/vigour-example",
  "dependencies": {
    "vigour-element": "~0.2.1",
    "vigour-statusbar": "^1.0.9",
    "vigour-uikit": "~0.3.1"
  },
  "devDependencies": {
    "vigour-wrapper": "^2.2.1",
    "vigour-ferry": "^3.0.1"
  },
  "gaston": {
    "package": true,
    "aliasify": {
      "uikit": "./node_modules/vigour-uikit"
    },
    "dist": {
      "files": [
        "build/web/**/*.*",
        "build/ios/vigour-native/www/**/*.*"
      ]
    }
  },
  "vigour": {
    "native": {
      "platforms": {
        "web": {
          "productName": "Vigourous example",
          "appIndexPath": "build.html",
          "assets": {
            "build.html": true,
            "build.css": true,
            "build.js": true
          }
        },
        "ios": {
          "location": "http://192.168.2.23:9090/",
          "productName": "Vigourous example",
          "organizationName": "Vigour",
          "organizationIdentifier": "io.vigour.ex",
          "buildNumber": "1",
          "appIndexPath": "index.html",
          "appUrlIdentifier": "io.vigour.example",
          "appUrlScheme": "cc",
          "appIcon": "assets/img/appIcon.png",
          "splashScreen": "assets/img/splash.png",
          "interfaceOrientations": {
            "iPhone": [
              "Portrait"
            ],
            "iPad": [
              "LandscapeLeft",
              "LandscapeRight"
            ]
          },
          "assets": {
            "build.js": true,
            "build.css": true,
            "assets": "*",
            "build.html": true
          }
        }
      }
    }
  }
}
